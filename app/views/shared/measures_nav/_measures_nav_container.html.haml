%div#measures_nav_container.col3
  = render 'shared/measures_nav/measures_nav_back_btn', node: node
  = render 'shared/measures_nav/measures_nav_current_node', node: node

  - unless node.measure?
    = render 'public_charts/overall_ranking_chart', node: node

  - if node.children
    - node.children.each do |child|
      = render 'shared/measures_nav/measures_nav_forward_btn', child: child
  - else
    = render 'shared/'

